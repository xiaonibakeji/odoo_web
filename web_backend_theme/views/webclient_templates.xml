<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="_assets_primary_variables" inherit_id="web._assets_primary_variables" priority="15">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/web_backend_theme/static/src/scss/primary_variables.scss"/>
        </xpath>
    </template>

    <template id="_assets_secondary_variables" inherit_id="web._assets_secondary_variables" priority="15">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/web_backend_theme/static/src/scss/secondary_variables.scss"/>
        </xpath>
    </template>

    <template id="assets_backend" name="web_backend_theme assets backend" inherit_id="web.assets_backend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/web_backend_theme/static/src/scss/navbar.scss"/>
            <link rel="stylesheet" type="text/scss" href="/web_backend_theme/static/src/scss/fields_extra.scss"/>
            <link rel="stylesheet" type="text/scss" href="/web_backend_theme/static/src/scss/form_view.scss"/>
            <link rel="stylesheet" type="text/scss" href="/web_backend_theme/static/src/scss/form_view_extra.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
        </xpath>
    </template>
</odoo>